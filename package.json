{
  "name": "tauri-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "build:template": "cd ./src-template && cargo build --release && cd ..",
    "build:template:debug": "cd ./src-template && cargo build && cd ..",
    "copy:template": "cp ./runner-go/WindowsFormsApp1.exe ./src-tauri/target/release/template.exe",
    "copy:template:debug": "cp ./runner-go/WindowsFormsApp1.exe ./src-tauri/target/debug/template.exe",
    "build:all": "tauri build && pnpm run build:template && pnpm run copy:template",
    "tauri:dev": "tauri dev && pnpm run copy:template"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.3",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@vueuse/core": "^13.1.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.1.5",
    "tailwindcss": "^4.1.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-vue": "^5.2.1",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vue-tsc": "^2.1.10"
  },
  "packageManager": "pnpm@8.15.3+sha256.fc4a49bd609550a41e14d20efbce802a4b892aa4cac877322de2f0924f122991"
}
